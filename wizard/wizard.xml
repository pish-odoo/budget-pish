<odoo>
<data>
      <record id="view_budget_wizard_form" model="ir.ui.view">
            <field name="name">budget.wizard.form</field>
            <field name="model">budget.wizard</field>
            <field name="arch" type="xml">
            <form>
                  <sheet>
                        <h2>Budget Name</h2>
                        <!-- <field name='name' placeholder='Budget: 1/1/2024 to 1/3/2024'/> -->
                        <group>
                              <group>
                                    <label for='start_date' string='Duration' />
                                    <div class="o_row">
                                    <field name='start_date' />
                                    <label for='end_date' string='->' />
                                    <field name='end_date' class='oe_inline' />
                                    </div>
                                    <field name='period' />
                              </group>
                        </group>
                  </sheet>
                  <footer>
                        <button string="Make an Offer" class="btn-primary" type="object" name="make_offer"/>
                        <button string="Cancel" class="oe_link" special="cancel"/>
                        <button string="Cancel" class="oe_link" special="cancel"/>
                  </footer>
            </form>
            </field>
      </record> 


      

